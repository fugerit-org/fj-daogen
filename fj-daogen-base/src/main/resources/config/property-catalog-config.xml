<data-catalog-config 
	check-duplicate-id="fail"
	default-catalog="comments">

	<data-list id="comments">
		<data id="common" description="Common code generation" xml="true"
			mode="cl" path="config/template/comments_common.xml"/>
		<data id="facade_def" description="Comment for facade definition" xml="true"
			mode="cl" path="config/template/comments_facade_def.xml"/>
		<data id="rest" description="Comment for rest" xml="true"
			mode="cl" path="config/template/comments_rest.xml"/>								
	</data-list>

	<data-list id="docgen">
		<data id="openapi" description="OpenAPI generation" xml="true"
			mode="cl" path="config/template/openapi_rest.xml"/>								
	</data-list>
	
</data-catalog-config>